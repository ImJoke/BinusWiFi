{
  "version": 2,
  "builds": [
    { "src": "/index.php", "use": "vercel-php@0.3.1" },
    { "src": "/public/**", "use": "@vercel/static" }
  ],
  "framework": null,
  "functions": {
    "index.php": {
      "runtime": "vercel-php@0.7.x"
    }
  },
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.php"
    }
  ]
}
