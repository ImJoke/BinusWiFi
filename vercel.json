{
    "version": 2,
    "functions": {
        "index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "builds": [
        {
            "src": "binusWifiAPI_prod.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "binusWifiAPI_prod.py"
        },
        {
            "src": "/",
            "dest": "index.php"
        }
    ]
}
