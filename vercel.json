{
    "version": 2,
    "builds": [
        {
            "src": "binusWifiAPI_prod.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.php",
            "use": "vercel-php@0.6.0"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "binusWifiAPI_prod.py"
        },
        {
            "src": "/",
            "dest": "index.php"
        }
    ]
}
