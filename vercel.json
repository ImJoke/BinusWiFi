{
  "version": 2,
  "builds": [
    {
      "src": "api/binusWifiAPI_prod.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.php",
      "use": "vercel/php@0.7.1"
    }
  ],
  "functions": {
    "index.php": {
      "includeFiles": "assets/**/*"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/binusWifiAPI_prod.py"
    },
    {
      "src": "/",
      "dest": "/index.php"
    }
  ]
}
